{
    "version": "1.0",
    "input": {
        "source": "samples/anon.png"
    },
    "output": {
        "destination": "temp/test1_output.png"
    },
    "operations": [
        {
            "type": "resize",
            "width": 800,
            "height": 450,
            "filter": "lanczos3"
        },
        {
            "type": "filter",
            "name": "grain",
            "intensity": 1.75
        },
        {
            "type": "filter",
            "name": "blur",
            "radius": 7.0
        },
        {
            "type": "filter",
            "name": "double_vision",
            "offset_x": 55,
            "offset_y": 0,
            "opacity": 0.5
        }
    ]
}